+	;;
+"friendlyelec,nanopi-r1" \
+|"friendlyelec,nanopi-r1s-h3" \
+|"friendlyelec,nanopi-r1s-h5")
+	ucidef_set_interfaces_lan_wan "eth1" "eth0"
